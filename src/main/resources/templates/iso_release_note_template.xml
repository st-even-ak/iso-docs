<div>
	<ac:structured-macro ac:name="panel" ac:schema-version="1">
	  <ac:parameter ac:name="title">System Description</ac:parameter>
	  <ac:rich-text-body>
	    <table class="wrapped">
	      <colgroup>
	        <col/>
	        <col/>
	      </colgroup>
	      <tbody>
	        <tr>
	          <th>JIRA ID</th>
	          <td>varJIRA</td>
	        </tr>
	        <tr>
	          <th>Author</th>
	          <td>$Author$</td>
	        </tr>
	        <tr>
	          <th>Server Details</th>
	          <td><at:var at:name="varServerDetails"/></td>
	        </tr>
	        <tr>
	          <th>Description</th>
	          <td><at:var at:name="wikiReleaseDescription" at:rawXhtml='true'/></td>
	        </tr>
	        <tr>
	          <th>Request Date</th>
	          <td>$varCreatedDate$</td>
	        </tr>
	        <tr>
	          <th>Requested By</th>
	          <td><at:var at:name="varProposer"/></td>
	        </tr>
	        <tr>
	          <th>Summary of Outcome from this Change</th>
	          <td><at:var at:name="wikiReleaseNoteSummary" at:rawXhtml='true'/></td>
	        </tr>
	        <tr>
	          <th>Potential Risks</th>
	          <td><at:var at:name="wikiReleaseNoteRisk" at:rawXhtml='true'/></td>
	        </tr>
	        <tr>
	          <th>Timescale for the Release to be Carried out</th>
	          <td><at:var at:name="varTimeScale"/></td>
	        </tr>
	        <tr>
	          <th>Method Statement</th>
	          <td><at:var at:name="wikiReleaseNoteMethodStatement" at:rawXhtml='true'/></td>
	        </tr>
	        <tr>
	          <th>Release Window</th>
	          <td><at:var at:name="varReleaseWindow"/></td>
	        </tr>
	      </tbody>
	    </table>
	  </ac:rich-text-body>
	</ac:structured-macro>
	<ac:structured-macro ac:name="panel" ac:schema-version="1">
	  <ac:parameter ac:name="title">Approval and Authorisation</ac:parameter>
	  <ac:rich-text-body>
	    <table class="wrapped">
	      <colgroup>
	        <col/>
	        <col/>
	      </colgroup>
	      <tbody>
	        <tr>
	          <th>Role</th>
	          <th>Name(s)</th>
	        </tr>
	        <tr>
	          <th>
	            <p class="ProcTableText">Client Approval</p>
	          </th>
	          <td>''USE Signer Macro</td>
	        </tr>
	        <tr>
	          <th>
	            <span>Technical Approval</span>
	          </th>
	          <td>
	            <br/>
	          </td>
	        </tr>
	        <tr>
	          <th>Solution Approval</th>
	          <td>
	            <br/>
	          </td>
	        </tr>
	      </tbody>
	    </table>
	  </ac:rich-text-body>
	</ac:structured-macro>
	<ac:structured-macro ac:name="panel" ac:schema-version="1">
	  <ac:parameter ac:name="title">Post Release Confirmation</ac:parameter>
	  <ac:rich-text-body>
	    <table class="wrapped">
	      <colgroup>
	        <col/>
	        <col/>
	        <col/>
	        <col/>
	      </colgroup>
	      <tbody>
	        <tr>
	          <th>Role</th>
	          <th>Summary of Action</th>
	          <th colspan="1">Completed?</th>
	          <th>
	            <span>Name(s)</span>
	          </th>
	        </tr>
	        <tr>
	          <td>
	            <br/>
	          </td>
	          <td>
	            <br/>
	          </td>
	          <td>

	          </td>
	          <td>
	            <br/>
	          </td>
	        </tr>
	      </tbody>
	    </table>
	  </ac:rich-text-body>
	</ac:structured-macro>
</div>